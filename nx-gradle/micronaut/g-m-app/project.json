{
  "name": "g-m-app",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "./nx-gradle/micronaut/g-m-app/src",
  "targets": {
    "build": {
      "executor": "@jnxplus/nx-gradle:run-task",
      "outputs": ["{projectRoot}/build"],
      "options": {
        "task": "build"
      }
    },
    "build-image": {
      "executor": "@jnxplus/nx-gradle:run-task",
      "options": {
        "task": "dockerBuild"
      }
    },
    "serve": {
      "executor": "@jnxplus/nx-gradle:run-task",
      "options": {
        "task": "run",
        "keepItRunning": true
      }
    },
    "test": {
      "executor": "@jnxplus/nx-gradle:run-task",
      "options": {
        "task": "test"
      }
    }
  },
  "tags": []
}
