{
  "name": "spring-boot-spring-boot-kt-app",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "./nx-gradle/spring-boot/spring-boot-kt-app/src",
  "targets": {
    "build": {
      "executor": "@jnxplus/nx-gradle:run-task",
      "outputs": ["{projectRoot}/build"],
      "options": {
        "task": "bootJar"
      }
    },
    "build-image": {
      "executor": "@jnxplus/nx-gradle:run-task",
      "options": {
        "task": "bootBuildImage"
      }
    },
    "serve": {
      "executor": "@jnxplus/nx-gradle:run-task",
      "options": {
        "task": "bootRun",
        "keepItRunning": true
      }
    },
    "test": {
      "executor": "@jnxplus/nx-gradle:run-task",
      "options": {
        "task": "test"
      }
    }
  },
  "tags": []
}
