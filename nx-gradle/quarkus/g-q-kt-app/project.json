{
  "name": "g-q-kt-app",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "./nx-gradle/quarkus/g-q-kt-app/src",
  "targets": {
    "build": {
      "executor": "@jnxplus/nx-gradle:run-task",
      "outputs": ["{projectRoot}/build"],
      "options": {
        "task": "quarkusBuild"
      }
    },
    "build-image": {
      "executor": "@jnxplus/nx-gradle:quarkus-build-image"
    },
    "serve": {
      "executor": "@jnxplus/nx-gradle:run-task",
      "options": {
        "task": "quarkusDev",
        "keepItRunning": true
      }
    },
    "test": {
      "executor": "@jnxplus/nx-gradle:run-task",
      "options": {
        "task": "test"
      }
    },
    "integration-test": {
      "executor": "@jnxplus/nx-gradle:run-task",
      "options": {
        "task": "quarkusIntTest"
      }
    }
  },
  "tags": []
}
